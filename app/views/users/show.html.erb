<p id="notice"><%= notice %></p>

<% if @message %>
  <%= @message %>
<% end %>

<p><strong><%= "ADMIN" if @user.admin %></strong></p>
<p><%= @user.name %></p>
<p>Tickets: <%= @user.tickets %></p>
<p>Mood: <%= @user.mood %></p>
<p>Height: <%= @user.height %></p>
<p><%= link_to 'Edit', edit_user_path(@user) %></p>
<p><%= link_to 'See attractions', attractions_path %></p>
